local module = {}

module["Name"] = "Ultimate Script"

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

-- Define the invisible variable
local invisible = false

-- Function to toggle invisibility
local function toggleInvisibility(character)
    if invisible then
        -- Make character invisible to all players
        for _, child in ipairs(character:GetChildren()) do
            if child:IsA("BasePart") then
                child.Transparency = 1
            end
        end
    else
        -- Make character visible to all players
        for _, child in ipairs(character:GetChildren()) do
            if child:IsA("BasePart") then
                child.Transparency = 0
            end
        end
    end
end

-- Module definition
module[1] = {
    Type = "Toggle",
    Args = {"Invisible Mod", function(Self)
        toggleInvisibility(Self)
    end}
}

-- Module definition
module[2] = {
       Type = "Button",
       Args = {"TEST", function(Self)
			if test then 
				fu.notification("Testing") 
				return 
			end
		end}
	}

_G.Modules = _G.Modules or {}
_G.Modules[#_G.Modules + 1] = module

return module
